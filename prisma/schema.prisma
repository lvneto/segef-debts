generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model Users {
  id                              Int     @id @default(autoincrement())
  register_type                   String?
  ua_jurisdiction                 String?
  name                            String?
  cnpj                            String?
  cpf                             String?
  cnae                            String?
  failed_date                     String?
  logradouro_type                 String?
  logradouro                      String?
  number                          String?
  complement                      String?
  zone                            String?
  city_code                       String?
  city                            String?
  state                           String?
  zip_code                        String?
  debts                           String?
  release_debts                   String?
  debt_type                       String?
  due_date                        String?
  umv                             String?
  main_debt                       String?
  apuration_period                String?
  declaration                     String?
  started_count_prescription_date String?
  process                         String?
  auto_violation                  String?
  release_check_date              String?
  article                         String? @db.VarChar(1024)
  extra_article                   String?
  mora_tax                        String?
  tax_due_date                    String?
  trade_tax_value                 String?

  users String? @db.VarChar(1024)
}
